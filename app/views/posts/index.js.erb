$('#index-container').append("<%= escape_javascript(render @posts, object: @posts) %>")
  setTimeout(function() {
		if($("#index-container").length){
			let magicGrid = new MagicGrid({
			container: '#index-container',
			animate: true,
			gutter: 30,
			static: true,
			useMin: true
		});
		magicGrid.listen();
		};
	}, 40);
$("#more_link").replaceWith("<%= escape_javascript(
  link_to_next_page(@posts, 'もっと見る', class: "btn white black-text", remote: true, id: 'more_link')
) %>")
